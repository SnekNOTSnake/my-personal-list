{
	"name": "my-personal-list",
	"description": "My personal anime tracking list",
	"version": "0.1.0",
	"scripts": {
		"start": "yarn start:renderer",
		"start:main": "NODE_ENV=development electron -r ts-node/register/transpile-only ./packages/main/index.ts",
		"start:renderer": "NODE_ENV=development ts-node --transpile-only scripts/watch.js",
		"dev": "node scripts/watch.mjs",
		"build": "yarn run typecheck && node scripts/build.mjs && electron-builder --config .electron-builder.config.js",
		"typecheck": "tsc --noEmit"
	},
	"private": true,
	"author": "Akhmad Najmuddin <udintobe@gmail.com>",
	"license": "MIT",
	"main": "dist/main/index.cjs",
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@types/react": "^17.0.33",
		"@types/react-dom": "^17.0.10",
		"@vitejs/plugin-react": "^1.0.7",
		"electron": "17.0.1",
		"electron-builder": "^22.14.13",
		"electron-devtools-installer": "^3.2.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.5.4",
		"vite": "^2.8.0",
		"vite-plugin-resolve": "^1.8.0"
	}
}
