{
	"name": "my-personal-list",
	"description": "My personal anime tracking list",
	"version": "1.4.0",
	"scripts": {
		"start": "concurrently \"yarn start:renderer\" \"yarn start:main\" --kill-others",
		"start:main": "tsc && electron .",
		"start:renderer": "vite dev",
		"build": "yarn run typecheck && yarn build:renderer && yarn build:main",
		"build:main": "NODE_ENV=production tsc --build",
		"build:renderer": "NODE_ENV=production vite build",
		"package:win": "electron-builder --win",
		"package:mac": "electron-builder --mac",
		"package:linux": "electron-builder --linux",
		"typecheck": "tsc --noEmit && tsc --noEmit --project packages/renderer",
		"test": "mocha"
	},
	"private": true,
	"author": "Akhmad Najmuddin <udintobe@gmail.com>",
	"homepage": "https://github.com/SnekNOTSnake/my-personal-list",
	"license": "MIT",
	"main": "dist/main/index.js",
	"dependencies": {
		"electron-store": "^8.1.0",
		"electron-updater": "^5.3.0",
		"nanoid": "^3.3.4",
		"prettier": "^2.8.0"
	},
	"devDependencies": {
		"@fontsource/roboto": "^4.5.8",
		"@types/mocha": "^10.0.0",
		"@types/prettier": "^2.7.1",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.9",
		"@vitejs/plugin-react": "^2.2.0",
		"chart.js": "^4.0.1",
		"concurrently": "^7.6.0",
		"electron": "21.3.1",
		"electron-builder": "^23.6.0",
		"electron-devtools-installer": "^3.2.0",
		"expect": "^29.3.1",
		"fuzzysort": "^2.0.4",
		"mocha": "^10.1.0",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.0.1",
		"react-dom": "^18.2.0",
		"react-icons": "^4.6.0",
		"react-router-dom": "^6.4.3",
		"recoil": "^0.7.6",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"vite-plugin-resolve": "^2.3.0"
	}
}
